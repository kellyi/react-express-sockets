{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "plugins": [
        "jest"
    ],
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1, "VariableDeclarator": 1 }],
        "no-console": 0,
        "arrow-parens": 0,
        "implicit-arrow-linebreak": 0,
        "operator-linebreak": 0,
        "object-curly-newline": 0
    }
}
